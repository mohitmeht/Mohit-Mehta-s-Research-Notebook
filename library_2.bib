@Article{Meini2013,
  Title                    = {{Rechargeability of Li-air cathodes pre-filled with discharge products using an ether-based electrolyte solution: implications for cycle-life of Li-air cells.}},
  Author                   = {Meini, Stefano and Tsiouvaras, Nikolaos and Schwenke, K Uta and Piana, Michele and Beyer, Hans and Lange, Lukas and Gasteiger, Hubert a},
