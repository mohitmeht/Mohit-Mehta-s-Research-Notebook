@Article{Nasybulin2013,
  Title                    = {{Effects of electrolyte salts on the performance of Li-O2~{b}atteries}},
  Author                   = {Nasybulin, Eduard and Xu, Wu and Engelhard, Mark H. and Nie, Zimin and Burton, Sarah D. and Cosimbescu, Lelia and Gross, Mark E. and Zhang, Ji Guang},
