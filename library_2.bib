@Article{Jorcin2010,
  Title                    = {{Investigation of the self-healing properties of shape memory polyurethane coatings with the 'odd random phase multisine' electrochemical impedance spectroscopy}},
  Author                   = {Jorcin, Jean Baptiste and Scheltjens, Gill and {Van Ingelgem}, Yves and Tourw??, Els and {Van Assche}, Guy and {De Graeve}, Iris and {Van Mele}, Bruno and Terryn, Herman and Hubin, Annick},
