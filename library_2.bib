@Article{Kokoschka2013,
  Title                    = {{Multiparametric characterization of nonelectroactive self-assembled monolayers during their formation.}},
  Author                   = {Kokoschka, Malte and Henry, John B and Bandarenka, Aliaksandr S},
  Journal                  = {Langmuir},
  Year                     = {2013},

