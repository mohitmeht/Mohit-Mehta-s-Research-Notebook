@Article{Levi2004b,
  author    = {Levi, M.D and Wang, C. and Aurbach, D.},
  title     = {Two parallel diffusion paths model for interpretation of PITT and EIS responses from non-uniform intercalation electrodes},
  journal   = {Journal of Electroanalytical Chemistry},
  year      = {2004},
