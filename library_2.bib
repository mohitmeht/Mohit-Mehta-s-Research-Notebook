@Article{Weidner1994,
  Title                    = {{Effect of Proton Diffusion, Electron Conductivity, and Charge-Transfer Resistance on Nickel Hydroxide Discharge Curves}},
  Author                   = {Weidner, John W and Timmerman, Paul},
  Journal                  = {Journal of The Electrochemical Society},
