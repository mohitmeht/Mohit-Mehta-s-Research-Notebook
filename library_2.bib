@Article{LaMantia2012,
  Title                    = {Dynamic response of thin-film semiconductors to {AC} voltage perturbations.},
  Author                   = {La Mantia, Fabio and Stojadinovi{\'c}, Jelena and Santamaria, Monica and Di Quarto, Francesco},
  Journal                  = {Chemphyschem},
