@Article{Park2006b,
  Title                    = {{Influence of anode diffusion layer properties on performance of direct borohydride fuel cell}},
  Author                   = {Park, Ki Tae and Jung, Un Ho and Jeong, Seong Uk and Kim, Sung Hyun},
  Journal                  = {Journal of Power Sources},
  Year                     = {2006},

  Month                    = nov,
  Number                   = {1},
  Pages                    = {192--197},
  Volume                   = {162},

  Abstract                 = {This study investigates the effect of the anode diffusion layer on the cell performance and stability of a direct borohydride fuel cell (DBFC). Carbon paper with various PTFE contents and thicknesses is used as the anode diffusion layer. In the case of the DBFC, both hydrogen evolution and liquid fuel diffusion have to be considered while selecting an anode diffusion layer. Among the various PTFE-containing diffusion layers, the non-contained diffusion layer yields the best performance. Adding PTFE to the diffusion layer leads to improved gas transport but is unsuitable in the DBFC. In addition, the thinnest (100$\mu$m) diffusion layer gives the best performance among the diffusion layers with various thicknesses. The decrease in the length of the diffusion path is beneficial for both the release of hydrogen gas and the diffusion of liquid fuel. In addition, it is found that the hydrogen evolution rate increases with increasing cell performance. Overall, adequate diffusion of liquid fuel is more important for the performance of a DBFC than the release of hydrogen gas.},
  Doi                      = {10.1016/j.jpowsour.2006.07.040},
  ISSN                     = {03787753},
  Keywords                 = {Anode diffusion layer,Diffusion layer thickness,Direct borohydride fuel cell,Hydrogen evolution,PTFE contents},
  Owner                    = {amnesia},
  Timestamp                = {2015.03.25},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0378775306012882}
}

