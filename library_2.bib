@Article{Xia2014,
  Title                    = {{Evolution of Li2O2 growth and its effect on kinetics of Li-O2~{b}atteries.}},
  Author                   = {Xia, Chun and Waletzko, Michael and Chen, Limei and Peppler, Klaus and Klar, Peter J and Janek, J\"{u}rgen},
  Journal                  = {ACS applied materials \& interfaces},
