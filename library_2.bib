@Article{Betova2010,
  Title                    = {{Estimation of kinetic and transport parameters by quantitative evaluation of EIS and XPS data}},
  Author                   = {Betova, I. and Bojinov, M. and Karastoyanov, V. and Kinnunen, P. and Saario, T.},
