@article{Franco2013c,
abstract = {This review focuses on the role of physical theory and computational
electrochemistry for fundamental understanding, diagnostics and design
of new electrochemical materials and operation conditions for energy
storage through rechargeable Li ion batteries (LIBs). More particularly,
deep insight based on multiscale physical modelling techniques, spanning
scales from few atoms to the device level, can advise about the materials
behaviour and aging and how components with optimal specifications
could be made and how they can be integrated into operating devices.
