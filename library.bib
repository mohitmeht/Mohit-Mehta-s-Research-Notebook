@article{Schweikert2011,
abstract = {We studied the behaviour of Li/Li(4)Ti(5)O(12) cells by electrochemical
impedance spectroscopy to gain insight into the changes at the electrode/electrolyte
interfaces during extensive cycling. A simple equivalent-circuit
model is able to describe the impedance of the complete battery as
a function of both state-of-charge and state-of-degradation. The
formation of the solid-electrolyte interface and dendrite growth
at the Li metal electrode have a strong influence on the impedance
measurements although the battery performance is not significantly
affected.},
