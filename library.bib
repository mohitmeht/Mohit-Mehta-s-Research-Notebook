number = {12},
pages = {2910--8},
pmid = {22566168},
title = {{Dynamic response of thin-film semiconductors to AC voltage perturbations.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22566168},
volume = {13},
year = {2012}
}
