@article{Chung2013,
abstract = {Nanostructured carbon-based materials, such as nitrogen-doped carbon
nanotube arrays, Co3O4/nitrogen-doped graphene hybrids and carbon
nanotube-graphene complexes have shown respectable oxygen reduction
reaction activity in alkaline media. Although certainly promising,
