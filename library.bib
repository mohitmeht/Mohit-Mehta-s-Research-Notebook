keywords = {Bioelectric Energy Sources,Catalysis,Charcoal,Charcoal: chemistry,Electrodes,Polytetrafluoroethylene,Polytetrafluoroethylene: chemistry},
month = {nov},
number = {17},
pages = {5777--5787},
pmid = {22939222},
publisher = {Elsevier Ltd},
title = {{A novel structure of scalable air-cathode without Nafion and Pt by rolling activated carbon and PTFE as catalyst layer in microbial fuel cells.}},
