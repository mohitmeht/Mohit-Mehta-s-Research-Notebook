issn = {1463-9084},
journal = {Phys. Chem. Chem. Phys.},
month = {mar},
number = {11},
pages = {3764--3771},
pmid = {23389737},
title = {{Evidence for lithium superoxide-like species in the discharge product of a Li-O2 battery.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23389737},
volume = {15},
year = {2013}
}
