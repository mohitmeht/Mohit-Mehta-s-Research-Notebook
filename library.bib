pages = {26929--26939},
pmid = {24489976},
title = {{Monitoring the Electrochemical Processes in the Lithium-Air Battery by Solid State NMR Spectroscopy.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3905693{\&}tool=pmcentrez{\&}rendertype=abstract},
