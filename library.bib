journal = {Journal of Physical Chemistry C},
month = {mar},
number = {11},
pages = {4019--4023},
publisher = {American Chemical Society},
title = {{Theory of anomalous diffusion impedance of realistic fractal electrode}},
url = {http://dx.doi.org/10.1021/jp712066k},
volume = {112},
year = {2008}
}
