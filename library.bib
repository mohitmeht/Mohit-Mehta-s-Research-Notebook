publisher = {Nature Publishing Group},
shorttitle = {Nat Chem},
title = {{Solvating additives drive solution-mediated electrochemistry and enhance toroid growth in non-aqueous Li-O2 batteries.}},
url = {http://dx.doi.org/10.1038/nchem.2132 http://www.nature.com/nchem/journal/v7/n1/abs/nchem.2132.html{\#}supplementary-information},
volume = {7},
year = {2014}
}
