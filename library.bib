@article{SINHA2007,
abstract = {Elevated temperature operation (above 80°C) of polymer electrolyte fuel cell (PEFC) is characterized by more difficult membrane hydration and low availability of oxygen to the cathode catalyst layer. Better membrane hydration and less resistance to oxygen transport are required for improved performance of PEFC, operated at elevated temperatures. Hence, diffusion media, being a gateway for reactant and product water transport between the gas channel and catalyst layer, require optimization to ensure an improved PEFC performance at elevated temperatures. In the present study, a functional dependence of cell voltage on diffusion media properties is characterized and a two-dimensional single-phase, non-isothermal model is used to study the effect of diffusion media properties on the performance of fuel cell operated at 95°C. It was found that there exists an optimum value of effective diffusive length of diffusion media that provides peak performance. In addition, a functionally graded diffusion media design is proposed that provides a better performance than conventional diffusion media at elevated temperature operation.},
author = {SINHA, P and WANG, C and SU, A},
doi = {10.1016/j.ijhydene.2006.10.066},
issn = {03603199},
journal = {International Journal of Hydrogen Energy},
keywords = {Diffusion media optimization,Elevated temperature operation,Functionally graded diffusion media,Polymer electrolyte fuel cell},
month = {may},
number = {7},
pages = {886--894},
title = {{Optimization of gas diffusion media for elevated temperature polymer electrolyte fuel cells}},
url = {http://www.sciencedirect.com/science/article/pii/S0360319906005386},
volume = {32},
