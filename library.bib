pages = {65--78},
title = {{CFD-based modelling of proton exchange membrane fuel cells}},
url = {http://www.sciencedirect.com/science/article/pii/S0378775304009589},
volume = {141},
year = {2005}
}
@article{Batteries2014,
abstract = {High energy efficiency and long cycleability are two important performance
measures for Li-air batteries. Using a rationally designed oxygen
