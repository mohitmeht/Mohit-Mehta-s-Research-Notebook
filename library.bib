@article{Viswanathan2014,
abstract = {Metal oxides are attractive candidates for low cost, earth-abundant
electrocatalysts. However, owing to their insulating nature, their
wide-spread application has been limited. Nanostructuring offers
a way around by enabling tunneling as a possible charge transport
mechanism. Using TiO2 as a model system for oxygen evolution reaction,
our theoretical analysis points to a critical thickness about {\~{}}
4 nm at a current density of {\~{}}1 mA/cm(2). This is corroborated
by electrochemical measurements on conformal thin films synthesized
using atomic layer deposition (ALD) identifying a similar critical
thickness. Finally, we generalize the theoretical analysis deriving
a relation between the critical thickness and the location of valence
band maximum relative to the limiting potential of the electrochemical
surface process. The critical thickness sets the optimum size of
the nano-particle oxide electrocatalyst and this provides an important
nano-structuring requirement for metal oxide electrocatalyst design.},
author = {Viswanathan, Venkatasubramanian and Pickrahn, Katie and Luntz, a. C and Bent, Stacey F and Norskov, Jens K},
doi = {10.1021/nl502775u},
issn = {1530-6992},
journal = {Nano Lett.},
keywords = {atomic layer deposition,charge transport,form key,lectrochemical and photoelectrochemical processes,nanostructuring,of renewable electricity in,routes toward the storage,such as,the form of,the splitting of water,to hydrogen and oxygen,water splitting},
month = {sep},
pmid = {25216362},
title = {{Nano-scale limitations in metal oxide electrocatalysts for oxygen evolution.}},
