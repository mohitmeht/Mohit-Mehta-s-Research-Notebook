journal = {Journal of Power Sources},
keywords = {Deposit layer,Li-O2 battery,Lithium peroxide,Model,Surface coverage},
month = {jan},
pages = {440--447},
title = {{Modeling of lithium–oxygen batteries with the discharge product treated as a discontinuous deposit layer}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0378775314015146 http://www.sciencedirect.com/science/article/pii/S0378775314015146},
volume = {273},
year = {2015}
