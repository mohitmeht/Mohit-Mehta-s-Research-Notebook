author = {Hartmann, Pascal and Gr{\"{u}}bl, Daniel and Sommer, Heino and Janek, J{\"{u}}rgen and Bessler, Wolfgang G. and Adelhelm, Philipp},
doi = {10.1021/jp4099478},
file = {:Users/mohitmehta/Copy/Mendley/2014/Pressure Dynamics in Metal–Oxygen (Metal–Air) Batteries A Case Study on Sodium Superoxide Cells - Hartmann et al. - The Journal of P.pdf:pdf},
