doi = {10.1021/jp5053453},
file = {:Users/mohitmehta/Copy/Mendley/2014/Simple Method to Relate Experimental Pore Size Distribution and Discharge Capacity in Cathodes for LiO 2 Batteries - Olivares-Mar{\'{\i}}n et.pdf:pdf},
issn = {1932-7447},
journal = {The Journal of Physical Chemistry C},
month = {sep},
