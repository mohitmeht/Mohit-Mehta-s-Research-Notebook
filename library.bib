issn = {1864-564X},
journal = {ChemSusChem},
keywords = {Electric Conductivity,Electric Power Supplies,Lithium,Lithium: chemistry,Magnetic Phenomena,Models, Molecular,Molecular Conformation,Oxygen,Oxygen: chemistry,Peroxides,Peroxides: chemistry,Quantum Theory,Surface Properties},
language = {en},
month = {jul},
