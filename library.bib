month = {oct},
number = {1},
pages = {146--160},
title = {{Gas diffusion layer for proton exchange membrane fuel cells-A review}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0378775309006399 http://www.sciencedirect.com/science/article/pii/S0378775309006399},
