journal = {ACS Applied Materials {\&} Interfaces},
month = {apr},
number = {14},
pages = {7670--7678},
publisher = {American Chemical Society},
title = {{Correlating Li/O 2 Cell Capacity and Product Morphology with Discharge Current}},
url = {http://pubs.acs.org/doi/abs/10.1021/acsami.5b00574},
