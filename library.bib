issn = {00220728},
journal = {Journal of Electroanalytical Chemistry},
keywords = {Reaction-diffusion impedance,Rough electrode,Self-affine fractal},
number = {SUPPL.},
pages = {141--151},
title = {{Effect of surface roughness on interfacial reaction-diffusion admittance}},
volume = {552},
year = {2003}
}
