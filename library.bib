pages = {1196--202},
pmid = {23670967},
publisher = {WILEY‐VCH Verlag},
title = {{Magnetism in lithium-oxygen discharge product.}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/cssc.201300223/full},
volume = {6},
year = {2013}
}
