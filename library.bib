issn = {22112855},
journal = {Nano Energy},
keywords = {Bi-functional catalysts,Durability,Graphene,Metal-air batteries,Perovskite oxide},
month = {nov},
pages = {192--200},
title = {{Electrospun porous nanorod perovskite oxide/nitrogen-doped graphene composite as a bi-functional catalyst for metal air batteries}},
url = {http://www.sciencedirect.com/science/article/pii/S2211285514200000},
volume = {10},
year = {2014}
}
