@article{Trahan2013,
abstract = {Dimethyl Sulfoxide (DMSO) was evaluated as a practical solvent for
the rechargeable lithium air battery. Redox characteristics of the
dissolved oxygen and its reduction products in the presence of lithium
hexafluorophosphate (LiPF6) supporting electrolyte were studied via
cyclic, rotating disk (RDE) and ring-disk (RRDE) electrode voltammetry.
