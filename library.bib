@article{Ogasawara2006,
abstract = {Rechargeable lithium batteries represent one of the most important
developments in energy storage for 100 years, with the potential
to address the key problem of global warming. However, their ability
to store energy is limited by the quantity of lithium that may be
removed from and reinserted into the positive intercalation electrode,
