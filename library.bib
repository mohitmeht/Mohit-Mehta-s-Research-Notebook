pages = {1513--1522},
title = {{Overview of current and future energy storage technologies for electric power applications}},
url = {http://www.sfu.ca/{~}mbahrami/ENSC 461/Project/Some papers for HRS/Overview of current and future energy storage technologies for electric power applications.pdf http://linkinghub.elsevier.com/retrieve/pii/S1364032108001664},
volume = {13},
year = {2009}
}
@article{Jung2012,
abstract = {Although dominating the consumer electronics markets as the power
source of choice for popular portable devices, the common lithium
battery is not yet suited for use in sustainable electrified road
transport. The development of advanced, higher-energy lithium batteries
