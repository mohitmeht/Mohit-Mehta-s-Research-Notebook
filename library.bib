doi = {10.1021/jp004132q},
file = {:Users/mohitmehta/Copy/Mendley/2001/Pulsed-gradient spin-echo 1H and 19F NMR ionic diffusion coefficient, viscosity, and ionic conductivity of non-chloroaluminate room-temp.pdf:pdf},
isbn = {1089-5647},
issn = {10895647},
journal = {Journal of Physical Chemistry B},
pages = {4603--4610},
title = {{Pulsed-gradient spin-echo 1H and 19F NMR ionic diffusion coefficient, viscosity, and ionic conductivity of non-chloroaluminate room-temperature ionic liquids}},
volume = {105},
year = {2001}
}
