issn = {15726657},
month = {jun},
pages = {22},
primaryClass = {cond-mat},
title = {{Asymptotic Analysis of Diffuse-Layer Effects on Time-Dependent Interfacial Kinetics}},
url = {http://arxiv.org/abs/cond-mat/0006104},
year = {2000}
}
