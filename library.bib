issn = {00220728},
journal = {Journal of Electroanalytical Chemistry},
keywords = {Ac Impedance spectroscopy,Carbon black,Colloidal suspension,Dispersant,Inverse micelles,Modeling},
pages = {67--78},
title = {{Characterization of non-aqueous dispersions of carbon black nanoparticles by electrochemical impedance spectroscopy}},
volume = {577},
year = {2005}
}
